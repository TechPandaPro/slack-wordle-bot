{
  "display_information": {
    "name": "Wordle"
  },
  "features": {
    "bot_user": {
      "display_name": "Wordle",
      "always_online": false
    },
    "slash_commands": [
      {
        "command": "/wordle",
        "description": "Play today's Wordle",
        "should_escape": false
      }
    ]
  },
  "oauth_config": {
    "scopes": {
      "bot": ["channels:history", "chat:write", "commands", "files:write"]
    }
  },
  "settings": {
    "event_subscriptions": {
      "bot_events": ["message.channels"]
    },
    "interactivity": {
      "is_enabled": true
    },
    "org_deploy_enabled": false,
    "socket_mode_enabled": true,
    "token_rotation_enabled": false
  }
}
